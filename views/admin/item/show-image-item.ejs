<div class="tab-pane fade show active" id="show-item" role="tabpanel" aria-labelledby="show-item-tab">
   <div class="row">
      <% items.imageId.forEach(imageId=> { %>
         <div class="col-md-4">
            <div class="card mb-4 mt-3 ">
               <div class="card-body ">
                  <img src="http://localhost:3000/<%= imageId.imageUrl %>" alt="Image" class="rounded mx-auto d-block"
                     style="width: 70%;
                     height: auto; display: block;">
               </div>
            </div>
         </div>
         <% }) %>
   </div>
   <a href="/admin/item" class="btn btn-warning mb-4">&laquo; Back</a>
</div>
<!-- DataTales Example -->